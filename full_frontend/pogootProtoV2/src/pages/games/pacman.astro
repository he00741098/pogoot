      columns[14 + i][10].isWall = true;
      columns[14][10 + i].isWall = true;
    }
    columns[15][11].isWall = true;

    for (var i = 0; i < 4; i++) {
      columns[26 + i][8].isWall = true;
      columns[29][8 + i].isWall = true;
    }
    for (var i = 0; i < 6; i++) {
      columns[26 + i][6].isWall = true;
      columns[31][6 + i].isWall = true;
    }

    for (var i = 0; i < 2; i++) {
      columns[26 + i][10].isWall = true;
      columns[26][10 + i].isWall = true;
    }
    columns[27][11].isWall = true;

    for (var i = 0; i < 3; i++) {
      columns[33 + i][6].isWall = true;
      columns[33 + i][11].isWall = true;
    }
    columns[33][7].isWall = true;
    columns[33][9].isWall = true;
    columns[33][10].isWall = true;
    for (var i = 0; i < 4; i++) {
      columns[35][7 + i].isWall = true;
    }

    for (var i = 0; i < 3; i++) {
      columns[37 + i][6].isWall = true;
      columns[37 + i][11].isWall = true;
    }

    for (var i = 0; i < 4; i++) {
      columns[37][7 + i].isWall = true;
    }
    for (var i = 0; i < 3; i++) {
      columns[39][8 + i].isWall = true;
      columns[41][8 + i].isWall = true;
    }
    columns[42][9].isWall = true;
    columns[41][11].isWall = true;
    columns[41][6].isWall = true;

    //generate points
    for (var i = 0; i < columns.length; i++) {
      for (var j = 0; j < columns[i].length; j++) {
        if (
          !columns[i][j].isWall &&
          !columns[i][j].isDoor &&
          !columns[i][j].containsPlayer
        ) {
          let random = Math.floor(Math.random() * 100);
          if (random > 90 && max_points > 0) {
            columns[i][j].containsBigPoint = true;
            max_points--;
          } else {
            columns[i][j].containsPoint = true;
          }
        }
      }
    }

    function draw_board() {
      ctx.clearRect(0, 0, 1100, 500);
      //ctx.save();

      //draw the board;
      for (var i = 0; i < columns.length; i++) {
        for (var j = 0; j < columns[i].length; j++) {
          if (columns[i][j].isWall) {
            ctx.fillRect(i * 25, j * 25, 25, 25);
          } else if (columns[i][j].isDoor) {
            ctx.fillStyle = "gray";
            ctx.fillRect(i * 25 + 13, j * 25, 12, 25);
            ctx.fillStyle = "black";
          } else if (columns[i][j].containsPlayer) {
            ctx.fillStyle = "yellow";
            ctx.fillRect(i * 25, j * 25, 25, 25);
            ctx.fillStyle = "black";
          } else if (columns[i][j].containsGhost) {
            if (buffed > 0) {
              ctx.fillStyle = "purple";
            } else {
              ctx.fillStyle = "red";
            }

            ctx.fillRect(i * 25 + 5, j * 25 + 5, 15, 15);
            ctx.fillStyle = "black";
          } else if (columns[i][j].containsPoint) {
            ctx.fillStyle = "white";
            ctx.fillRect(i * 25 + 10, j * 25 + 10, 5, 5);
            ctx.fillStyle = "black";
          } else if (columns[i][j].containsBigPoint) {
            ctx.fillStyle = "white";
            ctx.fillRect(i * 25 + 5, j * 25 + 5, 15, 15);
            ctx.fillStyle = "black";
          }
        }
      }
      //ctx.restore();
    }

    //44 x 20
    let player = {
      x: 21,
      y: 18,
      //N, E, S, W
      direction: 0,
      movement_direction: 0,
    };
    window.addEventListener("keydown", function (e) {
      if (e.key == "ArrowRight") {
        player.direction = 1;
      } else if (e.key == "ArrowUp") {
        player.direction = 0;
      } else if (e.key == "ArrowDown") {
        player.direction = 2;
      } else if (e.key == "ArrowLeft") {
        player.direction = 3;
      }
    });

    //fruit type = {type:0, life:25}
    let fruit = [];
    //ghost type = {color:0, x:21, y:4}
    let ghosts = [];
    //0 = red, 1 = blue, 2 = pink, 3 = green
    ghosts.push({ x: 20, y: 9, dead: 0 });
    ghosts.push({ x: 20, y: 9, dead: 0 });
    ghosts.push({ x: 20, y: 9, dead: 0 });
    ghosts.push({ x: 20, y: 9, dead: 0 });
    ghosts.push({ x: 20, y: 9, dead: 0 });
    for (var ghost of ghosts) {
      columns[ghost.x][ghost.y] = true;
    }

    function game_play() {
      //tick door
      if (door_open > 200) {
        door_open -= 120;
      } else {
        for (var i = 0; i < 4; i++) {
          columns[24][7 + i].isDoor = false;
        }
      }

      //calculate ghost movement
      for (var g of ghosts) {
        if (g.dead > 0) {
          g.dead -= 100;
          continue;
        }
        columns[g.x][g.y].containsGhost = false;
        let trying = true;
        while (trying) {
          let direction = Math.floor(Math.random() * 4);
          if (direction == 0 && g.y - 1 > 0 && !columns[g.x][g.y - 1].isWall) {
            g.y -= 1;
            trying = false;
          } else if (
            direction == 1 &&
            g.x + 1 < columns.length - 1 &&
            !columns[g.x + 1][g.y].isWall
          ) {
            g.x += 1;
            trying = false;
          } else if (
            direction == 2 &&
            g.y + 1 < columns[0].length - 1 &&
            !columns[g.x][g.y + 1].isWall
          ) {
            g.y += 1;
            trying = false;
          } else if (
            direction == 3 &&
            g.x - 1 > 0 &&
            !columns[g.x - 1][g.y].isWall &&
            !columns[g.x - 1][g.y].isDoor
          ) {
            g.x -= 1;
            trying = false;
          } else {
          }
        }

        columns[g.x][g.y].containsGhost = true;
      }

      window.requestAnimationFrame(draw_board);
      //check for ghost collisions
      if (columns[player.x][player.y].containsGhost) {
        if (buffed <= 100) {
          //console.log("collided: player position: ("+player.x+", "+player.y+")");
          //console.log(ghosts
          game_over();
          // window.requestAnimationFrame(draw_board);
          clearInterval(game_loop);
          return;
          // setInterval(function(){window.requestAnimationFrame(draw_board)}, 100);
        } else if (buffed > 0) {
          for (var ghost of ghosts) {
            if (ghost.x == player.x && ghost.y == player.y) {
              ghost.dead = 5000;
              ghost.x = 20;
              ghost.y = 9;
              columns[player.x][player.y].containsGhost = false;
            }
          }
        }
      }

      //unset moving pieces before calculating movement
      columns[player.x][player.y].containsPlayer = false;
      //determine direction
      if (player.movement_direction != player.direction) {
        //attempt to change direction
        if (
          player.direction == 0 &&
          player.y > 0 &&
          !columns[player.x][player.y - 1].isWall
        ) {
          player.movement_direction = 0;
        } else if (
          player.direction == 1 &&
          player.x < columns.length - 1 &&
          !columns[player.x + 1][player.y].isWall
        ) {
          player.movement_direction = 1;
        } else if (
          player.direction == 2 &&
          player.y < columns[0].length - 1 &&
          !columns[player.x][player.y + 1].isWall
        ) {
          player.movement_direction = 2;
        } else if (
          player.direction == 3 &&
          player.x > 0 &&
          !columns[player.x - 1][player.y].isWall &&
          !columns[player.x - 1][player.y].isDoor
        ) {
          player.movement_direction = 3;
        }
      }

      if (player.movement_direction == 0) {
        if (player.y > 0 && !columns[player.x][player.y - 1].isWall) {
          player.y -= 1;
        } else if (player.y <= 0) {
          player.y = 19;
        }
      } else if (player.movement_direction == 1) {
        if (
          player.x < columns.length - 1 &&
          !columns[player.x + 1][player.y].isWall
        ) {
          player.x += 1;
        } else if (player.x >= columns.length - 1) {
          player.x = 0;
        }
      } else if (player.movement_direction == 2) {
        if (
          player.y < columns[0].length - 1 &&
          !columns[player.x][player.y + 1].isWall
        ) {
          player.y += 1;
        } else if (player.y >= columns[0].length - 1) {
          player.y = 0;
        }
      } else if (player.movement_direction == 3) {
        if (
          player.x > 0 &&
          !columns[player.x - 1][player.y].isWall &&
          !columns[player.x - 1][player.y].isDoor
        ) {
          player.x -= 1;
        } else if (player.x <= 0) {
          player.x = 43;
        }
      }
      //consume points
      if (columns[player.x][player.y].containsPoint) {
        columns[player.x][player.y].containsPoint = false;
        points += 100;
      } else if (columns[player.x][player.y].containsBigPoint) {
        columns[player.x][player.y].containsBigPoint = false;
        document.getElementById("questionDialog").showModal();
        clearInterval(game_loop);
        points += 500;
        buffed = 10000;
        ask_question();
        return;
      }

      columns[player.x][player.y].containsPlayer = true;

      window.requestAnimationFrame(draw_board);
      //check for ghost collisions
      if (columns[player.x][player.y].containsGhost) {
        if (buffed <= 100) {
          //console.log("collided: player position: ("+player.x+", "+player.y+")");
          //console.log(ghosts);
          game_over();
          // window.requestAnimationFrame(draw_board);
          clearInterval(game_loop);

          return;
          // setInterval(function(){window.requestAnimationFrame(draw_board)}, 100);
        } else if (buffed > 0) {
          for (var ghost of ghosts) {
            if (ghost.x == player.x && ghost.y == player.y) {
              ghost.dead = 5000;
              ghost.x = 20;
              ghost.y = 9;
              columns[player.x][player.y].containsGhost = false;
            }
          }
        }
      }

      document.getElementById("score").innerText = points;
      buffed -= 100;
      window.requestAnimationFrame(draw_board);
    }

    function ask_question() {
      let question = Math.floor(Math.random() * questions.length);
      while (questions[question].seen) {
        question = Math.floor(Math.random() * questions.length);
      }
      questions[question].seen = true;

      document.getElementById("question").innerText =
        questions[question].question;
      let answer = people[questions[question].person];
      document.getElementById("0").onclick =
        document.getElementById("1").onclick =
        document.getElementById("2").onclick =
          function () {
            //console.log("you lose");
            game_over();
          };
      document.getElementById(questions[question].person).onclick =
        function () {
          game_loop = setInterval(game_play, 120);
          document.getElementById("questionDialog").close();
        };
    }

    let game_loop = setInterval(game_play, 120);

    function game_over() {
      document.getElementById("gameOver").showModal();
    }
  </script>


</html>
