      function airplane_slide() {
        document.getElementById("main").style.backgroundImage =
          "url('/images/presentation_1.png')";

        interval = setInterval(() => {
          // console.log("running");
          percent += 0.005;
          requestAnimationFrame(animate_plane);
          if (percent >= 1) {
