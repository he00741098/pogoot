        for (var p of path) {
          let segment_len =
            Math.pow(p.cx - point.cx, 2) + Math.pow(p.cy - point.cy, 2);
          segment_len = Math.pow(segment_len, 0.5);
          let angle = Math.atan(p.cy / p.cx);
          //segment time
          let segment_time = segment_len / velocity;
          if (time_passed <= t && t <= segment_time + time_passed) {
            return {
              found: true,
              cx: point.cx + velocity * Math.cos(angle) - 0.02,
              cy: point.cy + velocity * Math.sin(angle) - 0.02,
            };
          }
          point = p;
        }

        return { found: false };
      }

